{
  "appenders": {
    "out": {
      "type": "console"
    },
    "daily": {
      "type": "dateFile",
      "filename": "../logs/",
      "pattern": "yyyy-MM-dd.log",
      "alwaysIncludePattern": true
    }
  },
  "categories": {
    "default": {
      "appenders": ["daily", "out"],
      "level": "debug"
    }
  }
}
